#include <iostream>
#include <set>
#include <unordered_set>
using namespace std;

/*
 * Declare a Set
 * 	unordered_set<string> st;
 * 	set<string> st;
 *
 * FUNCTIONS 
 * 1. insert(value)
 * 	Add a value to the set
 * 	st.insert("apple");
 * 2. count(value)
 * 	Check if value exists
 * 	if(st.count("apple")) -> { } 
 * 3. erase(value)
 * 	Remove value from set
 * 	st.erase("apple");
 * 4. size()
 * 	Get number of elements
 * 	int n = st.size()
 * 5. empty()
 * 	Check if set is empty
 * 	if(st.empty()) -> { } 
 * 6. clear()
 * 	st.clear()
 * 7. find(value)
 * 	Returns an iterator to value or st.end()
 * 	if(st.find("apple") != st.end()){
 *		cout << "Found it!";
 * 	}
 * 8. Loop through set
 * 	for(auto& val : st){
 *		cout << val;
 * 	}
 * */

int main(){

	return 0;
}
